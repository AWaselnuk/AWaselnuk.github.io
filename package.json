{
  "name": "awaselnuk",
  "version": "0.1.0",
  "description": "Adam Waselnuk's jekyll blog.",
  "main": "gulpfile.js",
  "scripts": {
    "default": "npm run build && npm run server",
    "build": "npm run build:coffee && npm run build:sass && npm run build:jekyll",
    "build:jekyll": "jekyll build",
    "build:sass": "npm run sass && npm run autoprefix",
    "build:coffee": "coffee -c -o js/ _coffee/",
    "sass": "sass _scss/main.scss css/main.css",
    "autoprefix": "echo 'WARNING: autoprefixer not configured'",
    "server": "http-server _site/"
  },
  "author": "Shane Osbourne",
  "license": "ISC",
  "devDependencies": {
    "http-server": "^0.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shakyShane/jekyll-gulp-sass-browser-sync.git"
  },
  "keywords": [
    "jekyll",
    "gulp",
    "sass",
    "browsersync"
  ],
  "bugs": {
    "url": "https://github.com/shakyShane/jekyll-gulp-sass-browser-sync/issues"
  },
  "homepage": "https://github.com/shakyShane/jekyll-gulp-sass-browser-sync"
}
